{
  "devDependencies": {
    "autoprefixer": "^9.5.0",
    "bulma": "^0.7.2",
    "cpx": "^1.5.0",
    "parcel-bundler": "^1.12.2",
    "postcss-cli": "^6.1.2",
    "push-dir": "^0.4.1",
    "sass": "^1.17.3",
    "vue": "^2.6.10"
  },
  "alias": {
    "vue" : "./node_modules/vue/dist/vue.common.js"
  },
  "keywords": [
    "snet",
    "voting",
    "ethereum"
  ],
  "homepage": "snet-voting-ui",
  "license": "MIT",
  "name": "snet-voting-ui",
  "scripts": {
    "postcss": "npx postcss -u autoprefixer -r src/css/*",
    "scss": "npx node-sass --output-style compressed -o src/css src/scss",
    "serve": "npx parcel --no-cache --port 8080 src/index.html",
    "build": "npm run scss && npm run postcss && npx parcel build --no-cache src/index.html --public-url /snet-voting-ui",
    "deploy": "npx push-dir --dir=dist --branch=gh-pages"
  }
}
